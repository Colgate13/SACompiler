program
  // === TESTE DE STRESS E CASOS EXTREMOS ===
  
  // Declaração massiva de variáveis
  var int var1;
  var int var2;
  var int var3;
  var int var4;
  var int var5;
  var float f1;
  var float f2;
  var float f3;
  var string s1;
  var string s2;
  var string s3;
  var string s4;
  var string s5;
  
  // Números grandes e extremos
  var1 = 999999;
  var2 = 1;
  var3 = 0;
  var4 = 12345;
  var5 = 54321;
  
  // Floats com diferentes valores
  f1 = 0;
  f2 = 1;
  f3 = 123;
  
  // Strings de diferentes tamanhos
  s1 = "A";
  s2 = "Hello";
  s3 = "This_is_a_very_long_variable_name_test";
  s4 = "";
  s5 = "1234567890";
  
  print("=== TESTE DE NÚMEROS EXTREMOS ===");
  print(var1);
  print(var2);
  print(var3);
  
  // Expressão matemática extremamente complexa
  var int mega_calc;
  mega_calc = ((var1 + var2) * (var3 + var4)) / ((var5 - var1) + (var2 * var4)) + ((var3 * var5) - (var1 / var2));
  print("Cálculo mega complexo:");
  print(mega_calc);
  
  // Parenteses aninhados ao extremo
  var int nested_extreme;
  nested_extreme = (((var1 + var2) * (var3 - var4)) + ((var5 * var1) / (var2 + var3))) * (((var4 - var5) + (var1 * var2)) - ((var3 + var4) * (var5 - var1)));
  print("Aninhamento extremo:");
  print(nested_extreme);
  
  // Todos os operadores aritméticos juntos
  var int all_operators;
  all_operators = var1 + var2 - var3 * var4 / var5 % var1 + var2 - var3;
  print("Todos operadores:");
  print(all_operators);
  
  // Cadeia de condicionais extrema
  if (var1 > var2) {
    if (var2 > var3) {
      if (var3 < var4) {
        if (var4 != var5) {
          if (var5 >= var1) {
            print("Condicional aninhada LEVEL 5!");
          }
        }
      }
    }
  }
  
  // Teste de todos os operadores relacionais
  if (var1 == var1) { print("Igualdade: OK"); }
  if (var1 != var2) { print("Diferenca: OK"); }
  if (var1 > var2) { print("Maior: OK"); }
  if (var2 < var1) { print("Menor: OK"); }
  if (var1 >= var1) { print("Maior igual: OK"); }
  if (var1 <= var1) { print("Menor igual: OK"); }
  
  // Expressões com zeros
  var int zero_test1;
  var int zero_test2;
  zero_test1 = 0;
  zero_test2 = var1 * zero_test1;
  
  if (zero_test2 == 0) {
    print("Multiplicação por zero: OK");
  }
  
  // Teste de precedência complexa
  var int precedence_extreme;
  precedence_extreme = 1 + 2 * 3 - 4 / 2 + 5 * 6 - 7 + 8 % 3;
  print("Precedência extrema:");
  print(precedence_extreme);
  
  // Operações com strings extremas
  var string concat_test;
  concat_test = s1 + s2 + s3 + s4 + s5;
  print("Concatenação extrema:");
  print(concat_test);
  
  // Comparações de string extremas
  if (s1 != s2) {
    if (s2 != s3) {
      if (s3 != s4) {
        print("Strings todas diferentes!");
      }
    }
  }
  
  // Teste com números de um dígito vs multi-dígito
  var int single_digit;
  var int multi_digit;
  single_digit = 9;
  multi_digit = 987654321;
  
  if (multi_digit > single_digit) {
    print("Multi-dígito maior que single!");
  }
  
  // Expressão final ultra complexa
  var int final_boss;
  final_boss = (((var1 * var2) + (var3 - var4)) * ((var5 + f1) - (f2 * f3))) + (((single_digit * multi_digit) / (var1 + var2)) - ((var3 * var4) + (var5 - single_digit)));
  
  print("=== BOSS FINAL ===");
  print(final_boss);
  
end;