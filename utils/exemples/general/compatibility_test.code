program
  // === TESTE DE COMPATIBILIDADE TOTAL ===
  // Este exemplo testa TODOS os recursos da gramática
  
  // 1. Declaração de todos os tipos
  var int integer_value;
  var float float_value;
  var string string_value;
  
  // 2. Atribuições básicas
  integer_value = 42;
  float_value = 3;
  string_value = "Test";
  
  // 3. Print de todos os tipos
  print("=== TESTE DE TIPOS ===");
  print(integer_value);
  print(float_value);
  print(string_value);
  
  // 4. Todos os operadores aritméticos
  var int arith_test;
  arith_test = 10 + 5 - 2 * 3 / 2 % 4;
  print("Operadores aritméticos:");
  print(arith_test);
  
  // 5. Todos os operadores relacionais
  if (10 == 10) { print("Igualdade: OK"); }
  if (10 != 5) { print("Diferença: OK"); }
  if (10 > 5) { print("Maior: OK"); }
  if (5 < 10) { print("Menor: OK"); }
  if (10 >= 10) { print("Maior igual: OK"); }
  if (10 <= 10) { print("Menor igual: OK"); }
  
  // 6. Expressões com parênteses
  var int paren_test;
  paren_test = (10 + 5) * (8 - 3);
  print("Teste parênteses:");
  print(paren_test);
  
  // 7. Expressões aninhadas
  var int nested_test;
  nested_test = ((10 + 5) * (8 - 3)) / ((2 + 1) * (4 - 2));
  print("Teste aninhado:");
  print(nested_test);
  
  // 8. Condicionais aninhadas
  if (integer_value > 0) {
    if (float_value > 0) {
      if (string_value == "Test") {
        print("Aninhamento triplo: OK");
      }
    }
  }
  
  // 9. Concatenação de strings
  var string concat_result;
  concat_result = "Hello" + "World";
  print("Concatenação:");
  print(concat_result);
  
  // 10. Expressões complexas com múltiplas variáveis
  var int var_a;
  var int var_b;
  var int var_c;
  var int complex_result;
  
  var_a = 5;
  var_b = 10;
  var_c = 15;
  
  complex_result = (var_a * var_b) + (var_c / var_a) - (var_b % var_a);
  print("Expressão complexa:");
  print(complex_result);
  
  // 11. Teste de precedência total
  var int precedence_full;
  precedence_full = 1 + 2 * 3 - 4 / 2 + 5 % 3 * 2 - 1;
  print("Precedência completa:");
  print(precedence_full);
  
  // 12. Comparação entre variáveis
  if (var_a < var_b) {
    if (var_b < var_c) {
      print("Ordem crescente: OK");
    }
  }
  
  // 13. Expressão final mega complexa
  var int final_test;
  final_test = (((var_a + var_b) * var_c) - ((integer_value / float_value) + (arith_test % complex_result))) + (((paren_test * nested_test) / (var_a + var_b + var_c)) - ((precedence_full + 1) * (var_a - var_b + var_c)));
  
  print("=== TESTE FINAL ===");
  print(final_test);
  
  print("Todos os testes concluídos!");
  
end;